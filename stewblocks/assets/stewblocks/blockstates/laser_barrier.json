{
    "multipart": [
      {"apply": {"model": "stewblocks:laser_barrier_post"}},
      {
        "when": {"north": true},
        "apply": {"model": "stewblocks:laser_barrier_side"}
      },
      {
        "when": {"east": true},
        "apply": {"model": "stewblocks:laser_barrier_side", "y": 90}
      },
      {
        "when": {"south": true},
        "apply": {"model": "stewblocks:laser_barrier_side_alt"}
      },
      {
        "when": {"west": true},
        "apply": {"model": "stewblocks:laser_barrier_side_alt", "y": 90}
      },
      {
        "when": {"north": false},
        "apply": {"model": "stewblocks:laser_barrier_noside"}
      },
      {
        "when": {"east": false},
        "apply": {"model": "stewblocks:laser_barrier_noside_alt"}
      },
      {
        "when": {"south": false},
        "apply": {"model": "stewblocks:laser_barrier_noside_alt", "y": 90}
      },
      {
        "when": {"west": false},
        "apply": {"model": "stewblocks:laser_barrier_noside", "y": 270}
      }
    ]
  }